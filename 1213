#include <iostream>

using namespace std;

int main()
{
    string a;
    int f[16], pp = 0, i = 1;
    cin >> a;
    if (a.size() >= 8){
        pp += 1;
    }else{
        cout << "less than 8 characters" << endl;
    }
    while (i < a.size()){
        f += a.at(i);
        i += 1;
    }
    cout << a.at(1);
    
}
